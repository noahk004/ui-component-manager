{
    "name": "ui-component-manager",
    "version": "1.0.0",
    "description": "An online platform for users to share and download UI components made by the community.",
    "private": true,
    "scripts": {
        "dev": "turbo run dev --parallel --filter={./apps/*}",
        "build": "turbo run build --parallel --filter={./apps/*}",
        "start": "turbo run start --parallel --filter={./apps/*}",
        "lint": "turbo run lint --parallel --filter={./apps/*}"
    },
    "devDependencies": {
        "eslint": "^9",
        "prettier": "^3.4.2",
        "turbo": "^2.3.3"
    },
    "packageManager": "pnpm@9.12.2"
}
